{
  "name": "wireframe-svg-demo",
  "version": "0.0.0-development",
  "scripts": {
    "------------------ CORE COMMANDS -----------------": "",
    "ng": "ng",
    "------------------ LINT COMMANDS -----------------": "",
    "prettier": "prettier",
    "prettier:base": "yarn run prettier \"**/*.{js,json,scss,md,ts,html}\"",
    "prettier:write": "yarn run prettier:base --write",
    "prettier:check": "yarn run prettier:base --list-different",
    "------------------ DEMO COMMANDS -----------------": "",
    "demo:start": "yarn run ng serve",
    "demo:build:base": "yarn run ng build",
    "demo:build:prod": "yarn run demo:build:base --prod",
    "demo:test": "yarn run ng test",
    "demo:lint:check": "yarn run ng lint",
    "demo:lint:fix": "yarn run demo:lint:check --fix",
    "------------------ LIB COMMANDS -----------------": "",
    "lib:build:prod": "yarn run ng build --project wireframe-svg --prod",
    "lib:build:watch": "yarn run lib:build:prod --watch",
    "lib:test:watch": "yarn run ng test --project wireframe-svg",
    "lib:test:ci": "yarn run ng test --project wireframe-svg --watch false",
    "------------------ WASM COMMANDS ----------------": "",
    "wasm:build:prod": "wasm-pack build --out-dir dist/wireframe-svg/wasm-pkg",
    "wasm:lint:fix": "cargo fix --allow-dirty --allow-staged && cargo fmt",
    "wasm:lint:check": "cargo check && cargo fmt -- --check",
    "------------------ RELEASE COMMANDS ------------------": "",
    "semantic-release": "semantic-release",
    "------------------ QUICK COMMANDS ------------------": "",
    "lint:fix": "yarn demo:lint:fix && yarn prettier:write",
    "test": "yarn lib:test:watch",
    "start": "yarn demo:start"
  },
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/forms": "~9.0.3",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "@babylonjs/core": "^4.1.0",
    "core-js": "3.6.4",
    "observable-webworker": "^3.2.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "wasm-svg-from-wireframe": "file:./dist/wireframe-svg/wasm-pkg"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular-devkit/build-ng-packagr": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/language-service": "~9.0.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.4",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zakhenry/svg-from-wireframe.git"
  }
}
