{
  "name": "wireframe-svg-demo",
  "version": "0.0.0-development",
  "scripts": {
    "------------------ CORE COMMANDS -----------------": "",
    "ng": "ng",
    "------------------ LINT COMMANDS -----------------": "",
    "prettier": "prettier",
    "prettier:base": "yarn run prettier \"**/*.{js,json,scss,md,ts,html}\"",
    "prettier:write": "yarn run prettier:base --write",
    "prettier:check": "yarn run prettier:base --list-different",
    "------------------ DEMO COMMANDS -----------------": "",
    "demo:start": "yarn run ng serve",
    "demo:build:base": "yarn run ng build",
    "demo:build:prod": "yarn run demo:build:base --prod",
    "demo:test": "yarn run ng test",
    "demo:lint:check": "yarn run ng lint",
    "demo:lint:fix": "yarn run demo:lint:check --fix",
    "------------------ LIB COMMANDS -----------------": "",
    "lib:build:prod": "yarn run ng build --project wireframe-svg --prod",
    "lib:build:watch": "yarn run lib:build:prod --watch",
    "lib:test:watch": "yarn run ng test --project wireframe-svg",
    "lib:test:ci": "yarn run ng test --project wireframe-svg --watch false",
    "------------------ RELEASE COMMANDS ------------------": "",
    "semantic-release": "semantic-release",
    "------------------ QUICK COMMANDS ------------------": "",
    "lint:fix": "yarn demo:lint:fix && yarn prettier:write",
    "test": "yarn lib:test:watch",
    "start": "yarn demo:start"
  },
  "dependencies": {
    "@angular/animations": "~10.2.5",
    "@angular/common": "~10.2.5",
    "@angular/compiler": "~10.2.5",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~10.2.5",
    "@angular/platform-browser": "~10.2.5",
    "@angular/platform-browser-dynamic": "~10.2.5",
    "@angular/router": "~10.2.5",
    "@babylonjs/core": "^4.1.0",
    "core-js": "3.6.4",
    "mesh-to-svg": "2.1.6",
    "observable-webworker": "^3.2.3",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.4",
    "@angular-devkit/build-ng-packagr": "~0.1002.4",
    "@angular/cli": "~10.2.4",
    "@angular/compiler-cli": "~10.2.5",
    "@angular/language-service": "~10.2.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.4",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zakhenry/svg-from-wireframe.git"
  }
}